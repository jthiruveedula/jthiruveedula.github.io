{
  "name": "jthiruveedula-portfolio",
  "type": "module",
  "version": "2.0.0",
  "description": "Enterprise-grade portfolio website showcasing Jagadeesh Thiruveedula's data architecture expertise",
  "homepage": "https://jthiruveedula.github.io",
  "repository": {
    "type": "git",
